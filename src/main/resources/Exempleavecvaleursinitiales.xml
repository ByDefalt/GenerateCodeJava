<?xml version="1.0" encoding="UTF-8"?>
<Root model="#1" name="ExempleBateau">
    <Model id="#1" />

    <!-- Entité Point (utilisée pour initialiser position) -->
    <Entity id="#2" model="#1" name="Point" />
    <Attribute id="#3" entity="#2" name="x" type="Double" init="0.0" />
    <Attribute id="#4" entity="#2" name="y" type="Double" init="0.0" />

    <!-- Entité Bateau avec valeurs initiales -->
    <Entity id="#5" model="#1" name="Bateau" />
    <Attribute id="#6" entity="#5" name="position" type="Point" init="new Point(0, 0)" />
    <Attribute id="#7" entity="#5" name="nom" type="String" />
    <Attribute id="#8" entity="#5" name="id" type="Integer" init="nextId()" />
    <Attribute id="#9" entity="#5" name="valide" type="Boolean" init="false" />
    <Attribute id="#10" entity="#5" name="vitesse" type="Double" init="0.0" />
</Root>